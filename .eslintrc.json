{
 "parser": "babel-eslint",
 "parserOptions": {
   "ecmaVersion": 7,
     "sourceType": "module",
     "ecmaFeatures": {
     "jsx": true
   }
 },
 "extends": "eslint:recommended",
 "env": {
   "browser": true
 },
 "globals": {
   "module": true,
   "require": true,
   "exports": true
 },
 "rules": {
   "indent": [
     "error",
     2
   ],
   "linebreak-style": [
     "error",
     "unix"
   ],
   "semi": [
     "error",
     "always"
   ],
   "no-console": 0
 }
}